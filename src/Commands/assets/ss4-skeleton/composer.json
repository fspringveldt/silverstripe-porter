{
    "name": "$moduleName",
    "description": "Your descirption here",
    "type": "$moduleType",
    "require": {
        "silverstripe/framework": "^4.0",
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "license": "BSD-3-Clause",
    "scripts": {
        "lint": "phpcs -s code/ tests/"
    },
    "autoload": {
        "psr-4": {
            "$namespace": "src/",
            "$namespace\\Tests\\": "tests/PHP/"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
