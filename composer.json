{
  "name": "silverstripe/porter",
  "description": "Adds useful cli commands for the SilverStripe framework.",
  "keywords": [
    "module",
    "create",
    "cli",
    "SilverStripe",
    "create-module",
    "porter"
  ],
  "authors": [
    {
      "name": "Franco Springveldt",
      "email": "franco@silverstripe.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SilverStripe\\Porter\\": "src/",
      "SilverStripe\\Porter\\Tests\\": "tests/PHP"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "require": {
    "symfony/console": "^3.0",
    "symfony/filesystem": "^3.0",
    "symfony/process": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "lint": "phpcs -s code/ tests/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
